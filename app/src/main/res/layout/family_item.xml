<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="96dp"
    android:paddingTop="10dp"
    android:paddingBottom="10dp"
    android:paddingRight="19dp"
    android:paddingLeft="25dp">

    <ImageView
        android:layout_width="68dp"
        android:layout_height="68dp"
        android:id="@+id/img_family_icon"
        android:src="@drawable/icon_man"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginRight="15dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/self_info"
        android:id="@+id/txt_family_name"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/img_family_icon"
        android:layout_toEndOf="@+id/img_family_icon"
        android:textColor="@color/colorSecondaryText"
        android:textSize="13.2sp"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="13dp"
        android:id="@+id/imageView6"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:src="@drawable/navi_next"
        android:layout_centerVertical="true" />
</RelativeLayout>
